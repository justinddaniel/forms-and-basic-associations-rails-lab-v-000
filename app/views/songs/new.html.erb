<h1>Create a Song</h1>

<%= form_for @song  do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title %><br>
  <%= f.label :artist_name %>
  <%= f.text_field :artist_name %><br>
  <%= f.collection_select :genre, Genre.all, :id, :name %>
  <%= f.text_field :content %>
  <%= f.label :note_contents %>
  <%= f.text_field :note_contents %><br>
  <br />

  <%= f.submit %>
<% end %>
